{
  "name": "cultuurnet/entry",
  "description": "PHP library to access CultuurNet's Entry API",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Kristof Coomans",
      "email": "kristof@2dotstwice.be"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "cultuurnet/auth": "~1.1",
    "cultuurnet/cdb": "~2"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.0",
    "phpunit/phpunit": "~4.3"
  },
  "autoload": {
    "psr-4": {
      "CultuurNet\\Entry\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}
