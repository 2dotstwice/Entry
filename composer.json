{
  "name": "cultuurnet/entry",
  "description": "PHP library to access CultuurNet's Entry API",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Kristof Coomans",
      "email": "kristof@2dotstwice.be"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "cultuurnet/auth": "~1.2",
    "cultuurnet/cdb": "~2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "squizlabs/php_codesniffer": "~2.3",
    "phing/phing": "~2.11",
    "satooshi/php-coveralls": "~0.7",
    "guzzle/plugin-mock": "~3.9.2"
  },
  "autoload": {
    "psr-4": {
      "CultuurNet\\Entry\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}
